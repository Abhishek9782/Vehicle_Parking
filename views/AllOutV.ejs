<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Out Viechels</title>
    <link rel="icon" href="../frontimage/icon.webp">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
    <link rel="stylesheet" href="/stylee.css">
</head>

<body>
    <nav id="all">
        
        <div class="logo">Our Parking</div>
        
        
        <ul>
           
            <li><a href="/"> Home</a></li>
            <li><a href="/allentry" class="all">All Entry</a></li>
            <li><a href="/allreviews" >Reviews</a></li>
            <li><a href="/alloutViechele"  class="outactive">Out Viechels</a></li>
            <li><a href="" class="contact">Contact Us</a></li>
            <li><a href="/login">
                    <button id="logout" class="btn btn-light form-control">Log Out</button>
                </a></li>
        </ul>




</nav>

        <div style="background-color:black;">
            
            <button id="all-Parking" class="btn btn-primary">
                All Out 
                <span class="badge bg-secondary"><%=$_all_out%></span>
            </button>
            
     
        </div>
        <section id="mid" style="background-color:wheat ;">

            <div class="container">
                <div class="row">
                

                    <div class="col -md-10 mt-5 ">

                        <table class="table " id="mytable">
                            <tr style="color: white; text-align: center;">
                                <th>Sr No.</th>
                                <th>Date</th>
                                <th>VNo.</th>
                                <th>Viechel Types</th>
                                <th>Vin</th>
                                <th>Vout</th>
                                <th>Price</th>
                                <th>Status</th>
                                <th>Total Amount</th>
                                <th>Print</th>
                                




                            </tr>
                            <% let a=1;%>
                             <% alloutdata.forEach((result)=>{  %>

                          

                            <tr style="color: white; font-weight: bold;text-align: center">
                                <td><%= a++%></td>
                                <td><%= result.time%></td>
                                <td><%= result.vno%> </td>
                             
                                <td>  <%= result.vtype%> </td>
                                <td> <%= result.vin%> </td>
                                <td> <%= result.vout%> </td>
                                <td><%= result.price%></td>

                                <td> <%= result.status%></td>
                                <td><%= result.total%></td>
                                <td>
                                    <a href="/print/<%= result.id%>"><button class="btn btn-primary">Print..</button></a>
                                </td>

                            </tr>
                        <%  })%>  

                        </table>
                    </div>



                </div>
            </div>

        </section>

        <%- include('../partiels/footer.ejs')%>
</body>
<script>
    let z = 0;
    function abhishek() {
        const abhi = z++
        document.getElementById('all-Parking-button').innerText = abhi
    }





</script>

</html>